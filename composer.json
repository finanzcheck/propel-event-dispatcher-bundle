{
    "name": "finanzcheck/propel-event-dispatcher-bundle",
    "description": "Integrates the Propel EventDispatcherBehavior into Symfony.",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/finanzcheck/"
        },
        {
            "packagist.org": false
        }
    ],
    "require": {
        "php": "^8.0",
        "finanzcheck/propel-event-dispatcher-behavior": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0"
    },
    "authors": [
        {
            "name": "William DURAND",
            "email": "william.durand1@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Bazinga\\Bundle\\PropelEventDispatcherBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "Bazinga\\Bundle\\PropelEventDispatcherBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
